generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Roadmap {
  id          String   @id @default(uuid())
  title       String
  description String?  
  link        String
  createdAt   DateTime @default(now())
}

model Feedback {
  id        String   @id @default(uuid())
  name      String
  email     String
  message   String
  createdAt DateTime @default(now())
}
